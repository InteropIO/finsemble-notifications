{
  "components": {
    "notification-center": {
      "window": {
        "url": "$applicationRoot/components/finsemble-notifications/components/notification-center/notification-center.html",
        "frame": false,
        "resizable": true,
        "autoShow": true,
        "top": "center",
        "left": "center",
        "width": 800,
        "height": 800,
        "monitor": "primary"
      },
      "component": {
        "preload": false,
        "spawnOnStartup": true,
        "singleton": true
      },
      "foreign": {
        "services": {
          "dockingService": {
            "canGroup": true,
            "isArrangable": true
          }
        },
        "components": {
          "App Launcher": {
            "launchableByUser": true
          },
          "Window Manager": {
            "title": "notification-center",
            "FSBLHeader": true,
            "persistWindowState": true
          },
          "Toolbar": {
            "iconClass": "ff-component"
          }
        }
      }
    }
  }
}
