{
  "components": {
    "NotificationComponent": {
      "window": {
        "url": "$applicationRoot/components/finsemble-notifications/components/notify/notify.html",
        "affinity": "workspaceComponents",
        "frame": false,
        "resizable": true,
        "autoShow": true,
        "top": "center",
        "left": "center",
        "width": 400,
        "height": 432
      },
      "component": {
        "inject": false,
        "spawnOnStartup": false
      },
      "foreign": {
        "services": {
          "windowService": {
            "allowAutoArrange": true,
            "allowSnapping": true,
            "allowTabbing": true,
            "allowTiling": true,
            "allowGrouping": true
          }
        },
        "components": {
          "App Launcher": {
            "launchableByUser": true
          },
          "Window Manager": {
            "showLinker": true,
            "FSBLHeader": true,
            "persistWindowState": true,
            "title": "Notifications"
          },
          "Toolbar": {
            "iconClass": "ff-component"
          }
        }
      }
    }
  }
}
