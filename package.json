{
	"name": "finsemble-notifications",
	"version": "0.0.1",
	"description": "Notifications services and UI",
	"scripts": {
		"watch": "nodemon --delay 500ms -e js,ts,tsx,jsx,html,css,json copy.js",
		"copy-files": "node copy.js",
		"lint": "eslint services/** components/** ",
		"lint:fix": "eslint services/** components/** --fix ",
		"test": "mocha --color --bail",
		"test:watch": "nodemon --delay 500ms -e ts --exec \"mocha --color --bail\""
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx,md,html,css}": [
			"eslint --fix",
			"prettier --write",
			"git add"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ChartIQ/finsemble-notifications.git"
	},
	"author": "ChartIQ",
	"bugs": {
		"url": "https://github.com/ChartIQ/finsemble-notifications/issues"
	},
	"homepage": "https://github.com/ChartIQ/finsemble-notifications#readme",
	"dependencies": {
		"@types/react": "^16.9.11",
		"date-fns": "^2.8.1",
		"immutable": "^4.0.0-rc.12",
		"lodash.get": "^4.4.2",
		"searchjs": "^1.0.2",
		"uuid": "^3.3.3"
	},
	"devDependencies": {
		"@chartiq/finsemble": "^3.13.1",
		"@types/chai": "^4.2.7",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.12.5",
		"@babel/core": "^7.8.4",
		"@babel/preset-env": "^7.8.4",
		"@babel/preset-react": "^7.8.3",
		"@babel/register": "^7.8.3",
		"@testing-library/react": "^9.4.0",
		"@types/lodash.get": "^4.4.6",
		"@types/react-devtools": "^3.6.0",
		"@types/react-dom": "^16.9.3",
		"@typescript-eslint/eslint-plugin": "^2.10.0",
		"@typescript-eslint/parser": "^2.10.0",
		"ansi-html": "0.0.7",
		"chai": "^4.2.0",
		"eslint": "^6.7.2",
		"eslint-config-prettier": "^6.7.0",
		"eslint-config-react": "^1.1.7",
		"eslint-plugin-prettier": "^3.1.1",
		"eslint-plugin-react-hooks": "^2.3.0",
		"fs-extra": "^8.1.0",
		"husky": "^4.2.3",
		"lint-staged": "^10.0.7",
		"jsdom": "^16.1.0",
		"jsdom-global": "^3.0.2",
		"mocha": "^7.0.0",
		"nodemon": "^2.0.0",
		"prettier": "^1.19.1",
		"react": "16.8.4",
		"react-devtools": "^4.2.1",
		"react-dom": "16.8.4",
		"ts-node": "^8.6.2",
		"typescript": "^3.7.5"
	},
	"babel": {
		"presets": [
			"@babel/preset-env",
			"@babel/preset-react"
		]
	}
}
